            <input type="text" class="form-control" placeholder="Search...">
          </form>
        </div>
      </div>
    </nav>

    <div class="container-fluid">
      <div class="row">
       <jsp:include page="left.jsp"/>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <h1 class="page-header">Dashboard</h1>

          <div class="row placeholders">
            <div class="col-xs-6 col-sm-3 placeholder">
              <img data-src="holder.js/200x200/auto/sky" class="img-responsive" alt="200x200" src="data:image/svg+xml;base64," data-holder-rendered="true">
              <h4>Label</h4>
              <span class="text-muted">在线用户数 ${totalOneLineUser}</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="200x200" src="data:image/svg+xml;base64," data-holder-rendered="true">
              <h4>Label</h4>
              <span class="text-muted">在线登录数 ${totalLoginLineUser}</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img data-src="holder.js/200x200/auto/sky" class="img-responsive" alt="200x200" src="data:image/svg+xml;base64," data-holder-rendered="true">
              <h4>Label</h4>
              <span class="text-muted">今日注册用户数${todayTotal}</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="200x200" src="data:image/svg+xml;base64," data-holder-rendered="true">
              <h4>Label</h4>
              <span class="text-muted">今日使用房卡数${consumeTotal}</span>
            </div>
          </div>

          <h2 class="sub-header">在线房间一览</h2>
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>房间号</th>
                  <th>房主</th>
                  <th>房主昵称</th>
                  <th>局数</th>
                  <th>扎码数</th>
                  <th>创建时间</th>
                </tr>
              </thead>
              <tbody>
              	<c:forEach items="${roomList }" var="room">
              		<tr>
              			<td>${room.roomNumber }</td>
		                <td>${room.createUser.id }</td>
		                <td>${room.createUser.username }</td>
		                <td>${room.total }</td>
		                <td>${room.zhama }</td>
		                <td><fmt:formatDate value="${room.createDate}"  pattern="yyyy-MM-dd HH:mm:ss" /></td>
              		</tr>
              	</c:forEach>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>

