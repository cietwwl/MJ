package com.zxz.util;

public class StringUtil {

	/**判断字符串是否为空
	 * @param str
	 * @return
	 */
	public static boolean isNull(String str){
		if(str==null||"".equals(str)){
			return true;
		}
		return false;
	}
	
	
	/**得到int
	 * @param str
	 * @return
	 */
	public static int getInt(String str){
		try {
			return Integer.parseInt(str);
		} catch (NumberFormatException e) {
			return -1;
		}
	}
	
	
}
